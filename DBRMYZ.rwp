    <LUACode>function onInit()
	super.onInit();
	onValueChanged();	
	
end	


function onValueChanged()
	if window.getDatabaseNode().getChild("type").getValue() == '' then
		window.getDatabaseNode().getChild("type").setValue("gear");
	end
	
	if window.getDatabaseNode().getChild("type").getValue() == 'gear' then
		window.item_gear.setVisible(true);
		window.item_weapon.setVisible(false);
		window.item_armor.setVisible(false);			
	elseif window.getDatabaseNode().getChild("type").getValue() == 'weapon' then
		window.item_gear.setVisible(false);
		window.item_weapon.setVisible(true);
		window.item_armor.setVisible(false);			
	elseif window.getDatabaseNode().getChild("type").getValue() == 'armor' then
		window.item_gear.setVisible(false);
		window.item_weapon.setVisible(false);
		window.item_armor.setVisible(true);
	end
end

</LUACode>
