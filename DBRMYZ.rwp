function onBeforeDiceRoll()
	local nodeChar = window.getDatabaseNode();
	
	local nBase = DB.getValue(nodeChar, "dbase", 0);
	local nSkill = DB.getValue(nodeChar, "dskill", 0);
	local nGear = DB.getValue(nodeChar, "dgear", 0);

	local sDiceString = '';
	
	if nBase == 0 and nSkill == 0 and nGear == 0 then return false; end
	
	if nBase &gt; 0 then
		sDiceString = nBase .. "dbase+"
	end
	if nSkill &gt; 0 then
		sDiceString = sDiceString .. nSkill .. "dskill+"
	end
	if nGear &gt; 0 then
		sDiceString = sDiceString .. nGear .. "dgear"
	end

	DiceRollString = sDiceString;
	
	local diceInfoAttribute = DB.getValue(nodeChar, "diceInfoAttribute", "");
	local diceInfoSkill = DB.getValue(nodeChar, "diceInfoSkill", "");
	local diceInfoGear = DB.getValue(nodeChar, "diceInfoGear", "");
	DiceRollDescription = diceInfoSkill;
	if diceInfoAttribute ~= "" then
		DiceRollDescription = DiceRollDescription .. " (" .. diceInfoAttribute .. ")";
	end;
	if diceInfoGear ~= "" then
		DiceRollDescription = DiceRollDescription .. " (" .. diceInfoGear .. ")";
	end;

	DB.setValue(nodeChar, "diceInfof", "string", DiceRollDescription);
	DB.setValue(nodeChar, "diceInfoAttribute", "string", "");	
