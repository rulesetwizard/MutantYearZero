function update()
	local nodeRecord = getDatabaseNode();
	local bReadOnly = WindowManager.getReadOnlyState(nodeRecord);

	strength.setReadOnly(bReadOnly);
	agility.setReadOnly(bReadOnly);
	wits.setReadOnly(bReadOnly);
	empathy.setReadOnly(bReadOnly);
	ktzmod.setReadOnly(bReadOnly);

	for _,v in pairs(skills_list.getWindows(false)) do
		v.update(bReadOnly);
	end
	for _,v in pairs(mutations_list.getWindows(false)) do
		v.update(bReadOnly);
	end
	for _,v in pairs(gear_list.getWindows(false)) do
		v.update(bReadOnly);
	end
	for _,v in pairs(armor_list.getWindows(false)) do
		v.update(bReadOnly);
	end

	skills_list_iadd.setVisible(not bReadOnly);
	skills_list_iedit.setVisible(not bReadOnly);
	mutations_list_iadd.setVisible(not bReadOnly);
	mutations_list_iedit.setVisible(not bReadOnly);
	gear_list_iadd.setVisible(not bReadOnly);
	gear_list_iedit.setVisible(not bReadOnly);
	armor_list_iadd.setVisible(not bReadOnly);
	armor_list_iedit.setVisible(not bReadOnly);
end
