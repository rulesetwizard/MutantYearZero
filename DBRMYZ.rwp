	elseif sAttribute == "W" then nAttribute = DB.getValue(nodeChar, "wits", 0) - DB.getValue(nodeChar, "wits_loss", 0);  
	elseif sAttribute == "E" then nAttribute = DB.getValue(nodeChar, "empathy", 0) - DB.getValue(nodeChar, "empathy_loss", 0);  
	else return false; end
		
	if nAttribute &lt; 0 then nAttribute = 0; end
	if nSkill &lt; 0 then nSkill = 0; end
		
	DB.setValue(nodeChar, "dskill", "number", nSkill);
	DB.setValue(nodeChar, "dbase", "number", nAttribute);
		
	
	
end
