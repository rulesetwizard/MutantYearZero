    <LUACode>function onDoubleClick()
	local nodeWin = window.getDatabaseNode();
	local nodeChar = window.getDatabaseNode().getParent().getParent();
	local sDesc = DB.getText(nodeWin, "description");
	local sName = DB.getValue(nodeWin, "name");

	
	local msg = ChatManager.createBaseMessage(nodeChar, nil);
	
	local sIcon;
    if nodeChar.sType == "npc" then
        sIcon = "portrait_gm_token";
    else
        sIcon = string.gsub(DB.getPath(nodeChar), "charsheet.", "portrait_") .. "_chat";	
    end
    Debug.chat(sIcon); 
    msg.icon = sIcon;	
    
	local sMessage = sName .. "\n----------------------------------------\n" .. sDesc;
	
	msg.font = "chatblack";
	msg.mode = "chat_mutation";
	msg.text = sMessage;
	Comm.deliverChatMessage(msg);
	
	return true;
end
</LUACode>
