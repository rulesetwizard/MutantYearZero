    <LUACode>function onDoubleClick()
	local nodeMove = window.getDatabaseNode();
	local nodeChar = nodeMove.getParent().getParent();

	local nodeStat = getValue();
	local nodeName = window.name.getValue();
	local nodePool = DB.getValue(nodeChar, "dicepool", 0)
	DB.setValue(nodeChar, "dicepool", "number", nodeStat+nodePool)
	DB.setValue(nodeChar, "dicedesc", "string", DB.getValue(nodeChar, "dicedesc", '') .. nodeName  .. " [" .. nodeStat .. "], "); 
		
end
</LUACode>
