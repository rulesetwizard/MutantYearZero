    <LUACode>
function onDoubleClick(x, y)
	local nodeSkill = window.getDatabaseNode();
	local nodeChar = window.getDatabaseNode().getParent().getParent();
	local sAttribute = window.attribute.getValue();
	local sSkill = window.name.getValue();
	local nSkill = window.score.getValue();
	local nAttribute = 0;
	
	if sAttribute == "STR" then 
		nAttribute = DB.getValue(nodeChar, "strength", 0) - DB.getValue(nodeChar, "strength_loss", 0); 
	elseif 	sAttribute == "AGI" then 
		nAttribute = DB.getValue(nodeChar, "agility", 0) - DB.getValue(nodeChar, "agility_loss", 0);  
	elseif sAttribute == "WIT" then 
		nAttribute = DB.getValue(nodeChar, "wits", 0) - DB.getValue(nodeChar, "wits_loss", 0);  
	elseif sAttribute == "EMP" then 
		nAttribute = DB.getValue(nodeChar, "empathy", 0) - DB.getValue(nodeChar, "empathy_loss", 0);  
	else 
		return false; 
	end
		
	if nAttribute &lt; 0 then nAttribute = 0; end
	if nSkill &lt; 0 then nSkill = 0; end
		
	local nSkill1 = DB.getValue(nodeChar, "dskill", 0);
	local nBase1 = DB.getValue(nodeChar, "dbase", 0);
	
	DB.setValue(nodeChar, "dskill", "number", nSkill+nSkill1);
	DB.setValue(nodeChar, "dbase", "number", nAttribute+nBase1);
	DB.setValue(nodeChar, "diceInfoAttribute", "string", sAttribute);
	DB.setValue(nodeChar, "diceInfoSkill", "string", window.name.getValue());
	
end
</LUACode>
