function onDoubleClick(x, y)
	local nodeEntry = window.getDatabaseNode();
	local nodeChar = window.getDatabaseNode().getParent().getParent();
	local nGear = window.bonus.getValue();
	
	local nGear = DB.getValue(nodeChar, "dgear", 0);
	DB.setValue(nodeChar, "dgear", "number", nGear+getValue());
	local tGear = DB.getValue(nodeChar, "diceInfoGear", 0);
	if tGear == "" then
		DB.setValue(nodeChar, "diceInfoGear", "string", window.name.getValue());
	else
		DB.setValue(nodeChar, "diceInfoGear", "string", tGear .. ", " .. window.name.getValue());
	end;
	
end
