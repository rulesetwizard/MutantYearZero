end

function getDefaultSkill()
	local a_skill = data_skill.charskills;
--	Debug.chat("a_skill: ", a_skill);
	
	local entrymap = { };
	for _,w in pairs(getWindows()) do
		local sSkillName = w.name.getValue();
--		Debug.chat("54: ", sSkillName);
		entrymap[sSkillName] = w;
