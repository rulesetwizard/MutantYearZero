function onBeforeDiceRoll()
	local nodeChar = window.getDatabaseNode();
	
	
	local nBase = DB.getValue(nodeChar, "dbase", 0);
	local nSkill = DB.getValue(nodeChar, "dskill", 0);
	local nGear = DB.getValue(nodeChar, "dgear", 0);

	local sDiceString = '';
	
	if nBase == 0 and nSkill == 0 and nGear == 0 then return false; end
	
	if nBase &gt; 0 then
		sDiceString = nBase .. "dbase+"
	end
	if nSkill &gt; 0 then
		sDiceString = sDiceString .. nSkill .. "dskill+"
	end
	if nGear &gt; 0 then
		sDiceString = sDiceString .. nGear .. "dgear"
	end

	DiceRollString = sDiceString;

	DB.setValue(nodeChar, "dbase", "number", 0);	
	DB.setValue(nodeChar, "dskill", "number", 0);	
	DB.setValue(nodeChar, "dgear", "number", 0);	

	DB.setValue(nodeChar, "dbaser", "number", 0);	
	DB.setValue(nodeChar, "dskillr", "number", 0);	
	DB.setValue(nodeChar, "dgearr", "number", 0);	

	DB.setValue(nodeChar, "dbases", "number", 0);	
	DB.setValue(nodeChar, "dskills", "number", 0);	
	DB.setValue(nodeChar, "dgears", "number", 0);	

	DB.setValue(nodeChar, "dbasef", "number", 0);	
	DB.setValue(nodeChar, "dgearf", "number", 0);	
			
	return true;
end
