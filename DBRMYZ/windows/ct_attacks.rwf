function onDoubleClick(x, y)
	local nodeChar = window.getDatabaseNode();
	local nAttribute = getValue();
	local sLoss = getName() .. "_loss";
	local nLoss = DB.getValue(nodeChar, sLoss, 0);
	local nTotal = nAttribute - nLoss;
	if nTotal &lt; 0 then nTotal = 0; end
	
	DB.setValue(nodeChar, "dbase", "number", nTotal);
	
	
	
end
</LUACode>
