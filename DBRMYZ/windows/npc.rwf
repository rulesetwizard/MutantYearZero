    <LUACode>function onLockChanged()

	if npc_stats and npc_stats.subwindow then
		npc_stats.subwindow.update();
	end
	if npc_description and npc_description.subwindow then
		npc_description.subwindow.update();
	end

	local bReadOnly = WindowManager.getReadOnlyState(getDatabaseNode());
	name.setReadOnly(bReadOnly);
			
end

